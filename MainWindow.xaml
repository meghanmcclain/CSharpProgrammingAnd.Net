<Window x:Class="MidtermShipmentWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MidtermShipmentWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Cartons per Shipment:" HorizontalAlignment="Left" Margin="21,74,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="cartons" HorizontalAlignment="Left" Height="23" Margin="153,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Item per carton:" HorizontalAlignment="Left" Margin="19,125,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="books" HorizontalAlignment="Left" Height="23" Margin="153,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Total:" HorizontalAlignment="Left" Margin="66,252,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="total" HorizontalAlignment="Left" Height="23" Margin="153,256,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Calculate Total" HorizontalAlignment="Left" Margin="153,196,0,0" VerticalAlignment="Top" Width="100
                " Click="calculateClick"/>
    </Grid>
</Window>
